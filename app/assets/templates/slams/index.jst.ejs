<div class='slams-index-title'>
  <h3>Slams</h3>
</div>

<ul class='slams'>
  <% slams.each(function (slam) { %>
    <li>
      <a href="#/slams/<%= slam.id %>">
        <%= slam.escape("title") %>
        <%= slam.attributes.user.username %>
        <%= slam.escape("mode") %>
        <%= slam.escape("likes") %>
      </a>
    </li>
  <% }); %>
</ul>
